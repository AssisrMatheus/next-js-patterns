{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n next,codegen --restart-tries 3 --restart-after 3000 \"next dev\" \"graphql-codegen --config codegen.ts --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql.ts",
    "prisma": "prisma generate",
    "migrate": "npx prisma migrate dev"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@graphql-codegen/cli": "^2.13.1",
    "@graphql-codegen/client-preset": "^1.0.2",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@prisma/client": "^4.4.0",
    "@types/cors": "^2.8.12",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "apollo-server-micro": "^3.10.2",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "graphql": "^16.6.0",
    "micro": "^9.4.1",
    "micro-cors": "^0.1.1",
    "next": "12.3.1",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "prisma": "^4.4.0"
  }
}
