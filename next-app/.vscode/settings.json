{
  "saveAndRun": {
    "commands": [
      {
        "match": "**/*.{graphql,tsx,ts}",
        "cmd": "npm run codegen",
        "silent": true
      },
      {
        "match": "nexus/**/*.ts",
        "cmd": "npm run nexus",
        "silent": true
      },
      {
        "match": "prisma/schema.prisma",
        "cmd": "npm run prisma",
        "silent": true
      }
    ]
  }
}
